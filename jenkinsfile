pipeline {
    agent slave-2
    stages{
        stage("pull code"){
            steps{
                touch code
            }
         stage("Testing...."){
            steps{
                touch testing
            }
         stage("building...."){
            steps{
                touch build
            }
         stage("Deploying code....."){
            steps{
                touch deploy
            }
         stage("releasing...."){
            steps{
                touch release
            }
        }
    }
}
